{
  "defaultAssembly": "SimCivil",
  "components": [
    {
      "type": "SimCivil.Net.MatrixServer",
      "services": [
        {
          "type": "SimCivil.Net.IServerListener"
        },
        {
          "type": "SimCivil.ITicker"
        }
      ],
      "instanceScope": "singleinstance",
      "parameters": {
        "port": 20170,
        "ip": "127.0.0.1"
      }
    },
    {
      "type": "SimCivil.Store.JsonMapRepo",
      "services": [
        {
          "type": "SimCivil.Store.IMapRepository"
        },
        {
          "type": "SimCivil.Store.IPersistable"
        }
      ]
    },
    {
      "type": "SimCivil.Map.RandomMapGen",
      "services": [
        {
          "type": "SimCivil.Map.IMapGenerator"
        }
      ]
    },
    {
      "type": "SimCivil.Map.MapData",
      "instanceScope": "singleinstance"
    },
    {
      "type": "SimCivil.Store.ConfigRepository",
      "services": [
        {
          "type": "SimCivil.Store.IPersistable"
        }
      ]
    },
    {
      "type": "SimCivil.Auth.SimpleAuth",
      "services": [
        {
          "type": "SimCivil.Auth.IAuth"
        }
      ]
    },
    {
      "type": "SimCivil.Controller.PlayerController",
      "services": [
        {
          "type": "SimCivil.Controller.IController"
        },
        {
          "type": "SimCivil.ITicker"
        }
      ]
    }
  ]
}